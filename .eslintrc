{
  "extends": "loopback",
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2017
  },
  "rules": {
    "max-len": [
      "error",
      {
        "code": 120
      }
    ],
    "indent": [
      "error",
      4,
      {
        "SwitchCase": 1
      }
    ],
    "strict": [
      "error",
      "never"
    ],
    "no-underscore-dangle": 0,
    "curly": 0,
    "no-multi-spaces": 0,
    "key-spacing": 0,
    "no-return-assign": 0,
    "consistent-return": 0,
    "no-shadow": [
      "error",
      {
        "builtinGlobals": true
      }
    ],
    "camelcase": 1,
    "comma-dangle": [
      "error",
      "never"
    ],
    "new-cap": [
      "error",
      {
        "newIsCap": true,
        "capIsNew": false
      }
    ],
    "new-parens": 0,
    "no-use-before-define": 0,
    "no-empty": 0,
    "no-cond-assign": 0,
    "no-fallthrough": 0,
    "no-loop-func": 0,
    "no-unreachable": 0,
    "no-process-exit": 0,
    "no-useless-constructor": 0,
    "no-confusing-arrow": 0,
    "no-nested-ternary": 1,
    "no-unused-vars": [
      "error",
      {
        "vars": "all",
        "args": "after-used",
        "ignoreRestSiblings": false
      }
    ],
    "no-labels": 1,
    "no-trailing-spaces": 0,
    "prefer-const": [
      "error",
      {
        "destructuring": "any",
        "ignoreReadBeforeAssign": false
      }
    ],
    "object-shorthand": [
      "error",
      "always"
    ],
    "space-before-function-paren": [
      "error",
      "always"
    ],
    "spaced-comment": [
      "error",
      "always"
    ]
  }
}
