{
  "name": "Report",
  "base": "PersistedModel",
  "strict": true,
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "hidden": [],
  "mixins": {
    "TimeStamp": {
      "createdAt": "createdOn",
      "skipUpdatedAt": true,
      "required": false
    }
  },
  "properties": {
    "reason": {
      "type": "String",
      "required": true
    },
    "approvedOn": {
      "type": "Date"
    }
  },
  "validations": [],
  "relations": {
    "owner": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "ownerId"
    },
    "model": {
      "type": "belongsTo",
      "polymorphic": {
        "foreignKey": "modelId",
        "discriminator": "modelType"
      }
    },
    "approvedBy": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "approvedUserId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW",
      "property": [
        "find",
        "count",
        "upsert",
        "findById",
        "findOne",
        "exists",
        "deleteById",
        "destroyById",
        "__get__owner"
      ]
    }
  ],
  "methods": {}
}
