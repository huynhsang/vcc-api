{
  "name": "ActivityPoint",
  "base": "PersistedModel",
  "strict": true,
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "createdAt": "createdOn",
      "updatedAt": "updatedOn",
      "required": false
    }
  },
  "hidden": [],
  "protected": [],
  "properties": {
    "activityName": {
      "type": "string",
      "required": true
    },
    "points": {
      "type": "number",
      "required": true
    },
    "isReceiver": {
      "type": "boolean",
      "default": false
    },
    "isBonus": {
      "type": "boolean",
      "default": false
    }
  },
  "validations": [],
  "relations": {
    "owner": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "ownerId"
    },
    "activity": {
      "type": "belongsTo",
      "model": "Activity",
      "foreignKey": "activityId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": []
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "administrator",
      "permission": "ALLOW",
      "property": [
        "find",
        "count",
        "findById",
        "findOne",
        "exists",
        "deleteById",
        "destroyById"
      ]
    }
  ],
  "methods": {}
}
