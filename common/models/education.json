{
  "name": "Education",
  "base": "PersistedModel",
  "strict": true,
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "createdAt": "created",
      "updatedAt": "modified",
      "required": false
    }
  },
  "hidden": [],
  "properties": {
    "degrees": {
      "type": ["String"],
      "required": true
    },
    "fieldOfStudy": {
      "type": "String",
      "required": true
    },
    "fromYear": {
      "type": "Number",
      "required": true
    },
    "toYear": {
      "type": "Number",
      "required": true
    },
    "grade": {
      "type": "String",
      "required": false
    },
    "activitiesAndSocieties": {
      "type": "String",
      "required": false
    },
    "description": {
      "type": "String",
      "required": false
    },
    "isPublic": {
      "type": "Boolean",
      "required": true,
      "default": true
    },
    "school": {
      "type": "String"
    }
  },
  "validations": [],
  "relations": {
    "owner": {
      "model": "user",
      "type": "belongsTo",
      "foreignKey": "ownerId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "accessType": "READ",
      "permission": "ALLOW"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "accessType": "WRITE",
      "permission": "ALLOW",
      "property": ["create"]
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "accessType": "WRITE",
      "permission": "ALLOW"
    },
    {
      "principalType": "ROLE",
      "principalId": "admin",
      "accessType": "*",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
