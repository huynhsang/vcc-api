{
  "name": "SubCategory",
  "base": "PersistedModel",
  "strict": true,
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "createdAt": "created",
      "updatedAt": "modified",
      "required": false
    }
  },
  "hidden": [],
  "properties": {
    "slug": {
      "type": "String",
      "required": true
    },
    "nameEn": {
      "type": "String",
      "required": true
    },
    "nameVi": {
      "type": "String",
      "required": true
    },
    "questionsCount": {
      "type": "Number",
      "required": true,
      "default": 0
    },
    "type": {
      "type": "String",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "category": {
      "type": "embedsOne",
      "model": "Category",
      "property": "categoryItem",
      "required": true,
      "options": {
        "persistent": true,
        "validate": true
      }
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "accessType": "READ",
      "permission": "ALLOW"
    },
    {
      "principalType": "ROLE",
      "principalId": "admin",
      "accessType": "WRITE",
      "permission": "ALLOW"
    },
    {
      "principalType": "ROLE",
      "principalId": "admin",
      "accessType": "EXECUTE",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
