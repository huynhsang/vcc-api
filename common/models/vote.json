{
  "name": "Vote",
  "base": "PersistedModel",
  "strict": true,
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "hidden": [],
  "mixins": {
    "TimeStamp": {
      "createdAt": "created",
      "skipUpdatedAt": true,
      "required": false
    }
  },
  "validations": [],
  "properties": {},
  "relations": {
    "owner": {
      "type": "belongsTo",
      "model": "user",
