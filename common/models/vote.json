    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "administrator",
      "permission": "ALLOW",
      "property": [
        "find",
        "count",
        "upsert",
        "findById",
        "findOne",
        "exists",
        "deleteById",
        "destroyById",
        "__get__user",
