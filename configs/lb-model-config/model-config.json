{
    "_meta": {
        "sources": [
            "loopback/common/models",
            "loopback/server/models",
            "../../common/models"
        ],
        "mixins": [
            "loopback/common/mixins",
            "loopback/server/mixins",
            "../node_modules/loopback-ds-timestamp-mixin",
            "../../common/mixins",
            "./mixins"
        ]
    },
    "AccessToken": {
        "dataSource": "vccDS",
        "public": false
    },
    "ACL": {
        "dataSource": "vccDS",
        "public": false
    },
    "RoleMapping": {
        "dataSource": "vccDS",
        "public": false,
        "options": {
            "strictObjectIDCoercion": true
        }
    },
    "Role": {
        "dataSource": "vccDS",
        "public": false
    },
    "user": {
        "dataSource": "vccDS",
        "options": {
            "emailVerificationRequired": true,
            "realmRequired": true,
            "realmDelimiter": ":"
        },
        "public": true
    },
    "Email": {
        "dataSource": "emailDS"
    },
    "Answer": {
        "dataSource": "vccDS",
        "public": true
    },
    "Category": {
        "dataSource": "vccDS",
        "public": true
    },
    "Education": {
        "dataSource": "vccDS",
        "public": true
    },
    "Experience": {
        "dataSource": "vccDS",
        "public": true
    },
    "Image": {
        "dataSource": "vccDS",
        "public": true
    },
    "Notification": {
        "dataSource": "vccDS",
        "public": true
    },
    "Reputation": {
        "dataSource": "vccDS",
        "public": true
    },
    "Question": {
        "dataSource": "vccDS",
        "public": true
    },
    "SubCategory": {
        "dataSource": "vccDS",
        "public": true
    },
    "Vote": {
        "dataSource": "vccDS",
        "public": true
    },
    "Report": {
        "dataSource": "vccDS",
        "public": true
    },
    "Wallet": {
        "dataSource": "vccDS",
        "public": true
    },
    "Remove": {
        "dataSource": "transient",
        "public": true
    },
    "Badge": {
        "dataSource": "transient",
        "public": true
    }
}
