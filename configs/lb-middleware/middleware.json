{
  "initial:before": {
    "loopback#favicon": {}
  },
  "initial": {
    "compression": {},
    "cors": {
      "params": {
        "origin": true,
        "credentials": true,
        "maxAge": 86400
      }
    },
    "helmet#xssFilter": {},
    "helmet#frameguard": {
      "params": { "action": "deny" }
    },
    "helmet#hsts": {
      "params": {
        "maxAge": 0,
        "includeSubdomains": true
      }
    },
    "helmet#hidePoweredBy": {},
    "helmet#ieNoOpen": {},
    "helmet#noSniff": {},
    "helmet#noCache": {
      "enabled": false
    }
  },
  "session": {
    "body-parser#json": {
    },
    "body-parser#urlencoded": {
      "params": {
        "extended": true
      }
    }
  },
  "auth:before": {
    "../../server/middleware/set-locale": {}
  },
  "auth": {},
  "auth:after": {
    "../../server/middleware/auth": {}
  },
  "parse": {},
  "routes": {
    "loopback#rest": {
      "paths": [
        "${restApiRoot}"
      ]
    }
  },
  "files": {},
  "final": {
    "loopback#urlNotFound": {}
  },
  "final:after": {
    "../../server/middleware/error-logger": {},
    "strong-error-handler": {
      "params": {
        "safeFields": ["errorCode"],
        "log": false
      }
    }
  }
}
